map:basicQuery a rr:LogicalTable;
    rr:sqlQuery """SELECT 
    ID as id,
    questioncode as question_code,
    questionnaire as questionnairename,
    Baseline as baseline,
    T3w as three_weeks,
    T3m as three_months,
    T6m as six_months,
    T1j as one_year

    FROM breast_data;""";
    rdfs:label "";
    skos:definition "".
 

map:patient a rr:TriplesMap;
    rr:logicalTable map:basicQuery;
    rr:subjectMap [
        rr:template "patient_{id}";
        rr:class ncit:C16960; 
    ];
        rr:predicateObjectMap [
        rr:predicate roo:P100061; #has_identifier
        rr:objectMap [
            rr:parentTriplesMap map:identifier;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate ncit:C154523; #patient_reported
        rr:objectMap [
            rr:parentTriplesMap map:questionnaire;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ].
    
###########################################
# Add identifier                                
###########################################
map:identifier a rr:TriplesMap;
    rr:logicalTable map:basicQuery;
    rr:subjectMap [
        rr:template "patient/{id}";
        rr:class ncit:C25364; 
    ];
    rr:predicateObjectMap [
        rr:predicate roo:P100042; #has_value
        rr:objectMap [
            rr:column "ID"; 
            rr:datatype xsd:string;
        ];
    ].

###########################################
# Add questionnaire                                
###########################################
map:questionnaire a rr:TriplesMap;
    rr:logicalTable map:basicQuery;
    rr:subjectMap [
        rr:template "patient/{id}/questionnaire";
        rr:class ncit:C17048; #questionnaire
    ];
    rr:predicateObjectMap [
        rr:predicate roo:hasname; #has name
        rr:objectMap [
            rr:parentTriplesMap map:questionnairename;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];    
    rr:predicateObjectMap [
        rr:predicate roo:consistsof; #consistsof
        rr:objectMap [
            rr:parentTriplesMap map:question_code;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate roo:hasversion; 
        rr:objectMap [
            rr:parentTriplesMap map:version;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ];
        rr:predicateObjectMap [
        rr:predicate roo:haslanaguage;
        rr:objectMap [
            rr:parentTriplesMap map:language;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ].

